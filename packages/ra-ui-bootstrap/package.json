{
    "name": "ra-ui-bootstrap",
    "version": "1.0.0",
    "description": "UI Components for react-admin with Bootstrap v4",
    "files": [
        "*.md",
        "lib",
        "esm",
        "src"
    ],
    "main": "lib/index",
    "module": "esm/index.js",
    "author": "Emmanuel dos Santos",
    "license": "ISC",
    "scripts": {
        "build": "yarn run build-cjs && yarn run build-esm",
        "build-cjs": "rimraf ./lib && tsc",
        "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
        "watch": "rimraf ./lib && tsc --watch"
    },
    "devDependencies": {
        "@types/node": "^12.7.12",
        "@types/react": "^16.3.0",
        "@types/react-paginate": "^6.2.1",
        "cross-env": "^5.2.0",
        "enzyme": "~3.7.0",
        "enzyme-adapter-react-16": "~1.6.0",
        "file-api": "~0.10.4",
        "ignore-styles": "~5.0.1",
        "react": "^16.3.2",
        "react-dom": "^16.3.3",
        "react-test-renderer": "~16.3.1",
        "rimraf": "^2.6.2"
    },
    "peerDependencies": {
        "react": "^16.3.0",
        "react-dom": "^16.3.0"
    },
    "dependencies": {
        "antd": "^3.23.6",
        "autosuggest-highlight": "^3.1.1",
        "bootstrap": "^4.3.1",
        "classnames": "~2.2.5",
        "inflection": "~1.12.0",
        "lodash": "~4.17.5",
