        "react-router-redux": "~5.0.0-alpha.9",
        "react-transition-group": "^2.2.1",
        "reactstrap": "^8.0.1",
        "recompose": "~0.26.0",
        "redux": "~3.7.2",
        "redux-form": "~7.4.0",
        "typescript": "^3.6.4"
    }
